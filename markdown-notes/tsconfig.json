{
  "compilerOptions": {
    "jsx": "react-jsx",

    /* Language and Environment */
    "target": "es2016",

    /* Modules */
    "module": "commonjs",

    /* Emit */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "skipLibCheck": true
  }
}
